{
  "name": "@runeya/fronts-landing",
  "version": "1.0.3",
  "private": true,
  "description": "Runeya landing page",
  "type": "module",
  "repository": "https://github.com/runeya/runeya",
  "author": {
    "name": "runeya",
    "email": "contact@runeya.dev",
    "url": "https://runeya.dev"
  },
  "scripts": {
    "build": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/tailwind.output.css && vite build",
    "serve": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/tailwind.output.css && vite",
    "test": "",
    "dev": "vite",
    "tailwind": "npx tailwindcss -i ./src/assets/css/tailwind.css -o ./src/assets/css/tailwind.output.css --watch",
    "start": "concurrently \"yarn tailwind\" \"yarn dev\""
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@primevue/themes": "^4.2.5",
    "@runeya/common-retrigger-all-build": "workspace:*",
    "@types/compression": "^1",
    "@types/express": "^5",
    "@vitejs/plugin-vue": "^5.2.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.35",
    "sass": "^1.70.0",
    "tailwindcss": "^3.4.16",
    "vite": "^6.3.5",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-html": "^3.2.2",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.1"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "compression": "^1.8.0",
    "express": "^5.1.0",
    "glob": "^11.0.2"
  }
}
