  "scripts": {
    "serve": "HTTP_PORT=5456 VITE_HTTP_PORT=5456 NODE_ENV=DEV concurrently --kill-others \"npm run serve:front\" \"npm run serve:server\"",
    "build": "vite build",
    "launchExamples": "cd example/front/ && npm ci && cd ../server && npm ci && cd .. && node ../server/bin/www ./stack.js",
    "serve:front": "vite",
    "serve:server": "cd server && nodemon --trace-warnings bin/www $STACKFILE",
    "serve:production": "npm run build && HTTP_PORT=5456 node server/bin/www ./example/stack.js",
    "start": "server/bin/www",
    "test": "",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "main": "./background.js",
  "files": [
    "server/**/*",
    "typings/**/*",
    "modules/**/*",
    ".env.production",
    "package.json"
  ],
