{
  "name": "@clabroche/fronts-docs",
  "version": "1.17.9",
  "private": true,
  "description": "Monitor processes as a stack",
  "repository": "https://github.com/clabroche/stack-monitor",
  "author": {
    "name": "clabroche",
    "email": "corentinlabroche@gmail.com",
    "url": "https://corentinlabroche.fr/"
  },
  "scripts": {
    "serve": "vitepress dev docs",
    "build": "vitepress build docs",
    "test": ""
  },
  "main": "./background.js",
  "files": [
    "server/**/*",
    "typings/**/*",
    "modules/**/*",
    ".env.production",
    "package.json"
  ],
  "typings": "./typings/export.d.ts",
  "types": "./typings/export.d.ts",
  "devDependencies": {
    "@clabroche/common-retrigger-all-build": "workspace:*",
    "vitepress": "^1.0.0-rc.41"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bin": "server/bin/www",
  "keywords": [
    "stack-monitor",
    "stack",
    "microservices",
    "git",
    "github",
    "bitbucket",
    "npm",
    "process",
    "command",
    "cli",
    "monitor",
    "monitoring"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.0.2"
}
