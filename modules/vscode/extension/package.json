{
  "name": "@clabroche/modules-vscode-extension",
  "displayName": "stack-monitor",
  "private": true,
  "description": "Connect to your stack-monitor instance",
  "repository": "https://github.com/clabroche/stack-monitor",
  "publisher": "clabroche",
  "license": "ISC",
  "version": "2.0.5",
  "icon": "./resources/rocket.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "enabledApiProposals": [
    "extensionRuntime"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "stack-monitor.connect",
        "title": "StackMonitor: Connect to stack monitor"
      },
      {
        "command": "stack-monitor.log",
        "title": "StackMonitor: Create log"
      },
      {
        "command": "stack-monitor.log-with-name",
        "title": "StackMonitor: Create log with variable name"
      }
    ],
    "keybindings": [
      {
        "command": "stack-monitor.log",
        "key": "ctrl+k l",
        "mac": "cmd+k l",
        "when": "editorTextFocus"
      },
      {
        "command": "stack-monitor.log-with-name",
        "key": "ctrl+k i",
        "mac": "cmd+k i",
        "when": "editorTextFocus"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "stack-monitor",
          "title": "Stack Monitor",
          "icon": "resources/rocket.png"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "welcome",
        "contents": "[Connect to stack-monitor](command:stack-monitor.connect)",
        "when": "!stackMonitorAvailable"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "services-explorer",
          "name": "Stackmonitor services",
          "visibility": "visible",
          "type": "tree",
          "when": "stackMonitorAvailable"
        }
      ],
      "stack-monitor": [
        {
          "id": "welcome",
          "name": "Welcome",
          "when": "!stackMonitorAvailable"
        },
        {
          "id": "services",
          "name": "Services",
          "visibility": "visible",
          "type": "tree",
          "when": "stackMonitorAvailable"
        },
        {
          "id": "dependencies",
          "name": "Dependencies",
          "type": "tree",
          "when": "stackMonitorAvailable"
        }
      ]
    }
  },
  "scripts": {
    "build": "node build.js",
    "test": ""
  },
  "dependencies": {
    "axios": "^1.7.9",
    "bluebird": "^3.7.2",
    "command-exists": "^1.2.9",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/command-exists": "^1.2.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@types/ws": "^8",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.20.1",
    "typescript": "^5.3.3"
  }
}
