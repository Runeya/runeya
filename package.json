{
  "name": "@runeya/runeya",
  "version": "1.0.0",
  "description": "",
  "main": "stack.js",
  "scripts": {
    "prepare": "husky",
    "postinstall": "husky install && node prepare-monorepo.js || true",
    "serve": "cross-env RUNEYA_HTTP_PORT=6874 runeya",
    "check:monorepo": "cd common/check-monorepo && npm run start",
    "check:dependencies-consistency": "check-dependency-version-consistency .",
    "check:unused": "knip",
    "check:all": "yarn check:unused && yarn check:monorepo && yarn check:dependencies-consistency",
    "plugin:create": "node common/plugin-creator/src/index.js",
    "ci:generate": "node ./common/workflows/src/index.js",
    "plugin:dev": "turbo run plugin:dev  --env-mode=loose",
    "plugin:publish": "turbo run plugin:publish --env-mode=loose",
    "lint": "turbo run lint  --env-mode=loose",
    "test": "turbo run test  --env-mode=loose",
    "build": "turbo run build --filter @runeya/servers-server --env-mode=loose",
    "docs:build": "turbo run build --filter @runeya/fronts-docs --env-mode=loose",
    "eslint": "eslint",
    "turbo": "turbo",
    "lerna": "npx lerna",
    "changed": "yarn lerna changed -pl --toposort"
  },
  "repository": "git+https://github.com/runeya/runeya.git",
  "author": "runeya <contact@runeya.dev>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/runeya/runeya/issues"
  },
  "homepage": "https://github.com/runeya/runeya#readme",
  "workspaces": {
    "packages": [
      "./fronts/*",
      "./modules/*/*",
      "./common/*",
      "./servers/*",
      "./plugins/*"
    ]
  },
  "private": true,
  "packageManager": "yarn@4.9.2",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@lerna-lite/changed": "^2.5.0",
    "@lerna-lite/cli": "^2.5.0",
    "@lerna-lite/diff": "^2.5.0",
    "@lerna-lite/exec": "^2.5.0",
    "@lerna-lite/list": "^2.5.0",
    "@lerna-lite/publish": "^2.5.0",
    "@lerna-lite/version": "^2.5.0",
    "@runeya/common-retrigger-all-build": "workspace:*",
    "@runeya/common-typings": "workspace:^",
    "@runeya/runeya": "^1.33.27",
    "@types/bluebird": "^3.5.38",
    "@types/debounce": "^1",
    "@types/lodash": "^4.14.201",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "bluebird": "^3.7.2",
    "check-dependency-version-consistency": "^4.1.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "debounce": "^2.0.0",
    "dotenv": "^16.4.2",
    "eslint": "^9.17.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vue": "^9.32.0",
    "fix-esm": "^1.0.1",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "knip": "^5.60.2",
    "tsup": "^8.5.0",
    "typescript": "^5.3.3",
    "workspace-tools": "^0.36.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "turbo": "^2.3.3 "
  }
}
