{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "plugin:dev": {
      "dependsOn": [
        "^plugin:dev"
      ],
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        ".vitepress/dist/**",
        "dist/**",
        "out/**",
        "build/**",
        "server/public/**"
      ]
    },
    "test": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": [
        "build",
        "test",
        "lint"
      ],
      "outputs": []
    }
  }
}